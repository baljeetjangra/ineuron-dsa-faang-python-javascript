    "def possibilities(n):\n",
    "    if n == 2:\n",
    "        return 2\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        ## recursion \n",
    "        return possibilities(n-1) + possibilities(n-2)\n",
    "## Driver code\n",
    "n = 8\n",
    "result = possibilities(n)\n",
    "print(result)"
   ]
